		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/node": "24.7.0",
		"@types/service-worker-mock": "^2.0.4",
		"@vitest/ui": "^3.2.4",
		"drizzle-kit": "^0.31.5",
		"husky": "^9.1.7",
		"vitest": "^3.2.4",
		"wrangler": "^4.43.0"
	}
