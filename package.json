		"add": "^2.0.6",
		"ai": "^6.0.0-beta.133",
		"chanfana": "^2.8.3",
		"drizzle-orm": "^0.44.7",
		"drizzle-zod": "^0.8.3",
		"hono": "^4.10.7",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/node": "24.7.0",
		"@types/service-worker-mock": "^2.0.4",
		"@vitest/ui": "^3.2.4",
		"drizzle-kit": "^0.31.8",
